//Declarative approach

pipeline {
	agent any
	stages {
		stage ('Build') {
			steps {
				echo "Build"
			}

		}
		
	}
}

pipeline {
	agent any
	stages {
		stage ('Test') {
			steps {
				echo "Test"
			}

		}
		
	}
}

pipeline {
	agent any
	stages {
		stage ('Integration') {
			steps {
				echo "Integration"
			}

		}
		
	}
}